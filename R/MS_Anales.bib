  file = {/Users/tomasmarina/Zotero/storage/ISAHW3LK/Rodriguez et al. - 2022 - Marine food webs are more complex but less stable .pdf}
}

@article{Rossi2019,
  title = {Antarctic Food Web Architecture under Varying Dynamics of Sea Ice Cover},

  shorttitle = {Regime Shifts in Marine Communities},
  author = {Yletyinen, Johanna and Bodin, {\"O}rjan and Weigel, Benjamin and Nordstr{\"o}m, Marie C. and Bonsdorff, Erik and Blenckner, Thorsten},
